{
  "excelwrite": {
    "prefix": "excelwrite",
    "body": [
      "with pd.ExcelWriter(${1:path}, engine='openpyxl') as writer:",
      "    df.to_excel(writer, sheet_name='paint')",
      "    $0"
    ],
    "description": "add excel sheet write to multiple tab"
  },
  "jsonopen": {
    "prefix": "jsonopen",
    "body": [
      "with open(${1:path}, 'r') as file:",
      "    data = json.load(file)",
      "    $0"
    ],
    "description": "open json file"
  },
  "jsonwrite": {
    "prefix": "jsonwrite",
    "body": [
      "with open(${1:path}, 'w', encoding='utf-8') as file:",
      "    json.dump(data, f, ensure_ascii=False, indent=2)",
      "    $0"
    ],
    "description": "write json file"
  }
}
